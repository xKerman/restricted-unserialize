{
    "name": "xkerman/restricted-unserialize",
    "description": "provide PHP Object Injection safe unserialize function",
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpmd/phpmd": "@stable",
        "phpunit/phpunit": "^5.5",
        "sebastian/phpcpd": "^2.0",
        "squizlabs/php_codesniffer": "2.*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "xKerman",
            "email": "xKhorasan@gmail.com"
        }
    ],
    "autoload": {
        "files": ["src/function.php"],
        "psr-4": {
            "xKerman\\Restricted\\": "src"
        }
    },
    "scripts": {
        "test": [
            "phpcs -sw --standard=psr2 src/ test/",
            "phpmd src/,test/ text ./phpmd.xml",
            "phpcpd src/",
            "phpunit"
        ]
    }
}
